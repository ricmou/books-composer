		{
			"name": "Get Book",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"Value Testing\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.id).to.eql(\"978-6666666666\");\r",
							"    pm.expect(jsonData.title).to.eql(\"Test1\");\r",
							"    pm.expect(jsonData.language).to.eql(\"es-MX\");\r",
							"    pm.expect(jsonData.description[0].text).to.eql(\"Test1\");\r",
							"    pm.expect(jsonData.description[0].language).to.eql(\"en-GB\");\r",
							"    pm.expect(jsonData.publisher.publisherId).to.eql(\"TE1\");\r",
							"    pm.expect(jsonData.publisher.name).to.eql(\"Test oen\");\r",
							"    pm.expect(jsonData.publisher.country).to.eql(\"DK\");\r",
							"    pm.expect(jsonData.authors[0].authorId).to.eql(\"TE1\");\r",
							"    pm.expect(jsonData.authors[0].firstName).to.eql(\"Test\");\r",
							"    pm.expect(jsonData.authors[0].lastName).to.eql(\"One\");\r",
							"    pm.expect(jsonData.authors[0].birthDate).to.eql(\"01/05/1990\");\r",
							"    pm.expect(jsonData.authors[0].country).to.eql(\"US\");\r",
							"    pm.expect(jsonData.categories[0].categoryId).to.eql(\"TE1\");\r",
							"    pm.expect(jsonData.categories[0].name).to.eql(\"Test1\");\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
